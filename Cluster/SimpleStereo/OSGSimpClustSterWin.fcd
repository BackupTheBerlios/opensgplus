<?xml version="1.0"?>

<FieldContainer
	name="SimpClustSterWin"
	parent="ClusterWindow"
	library="Cluster"
	pointerfieldtypes="none"
	structure="concrete"
	systemcomponent="true"
	parentsystemcomponent="true"
>
Cluster rendering configuration for a two server stereo display
	<Field
		name="fov"
		type="Real32"
		cardinality="single"
		visibility="external"
		access="public"
	>
	The vertical field of view, in radians.
	</Field>
	<Field
		name="eyedistance"
		type="Real32"
		cardinality="single"
		visibility="external"
		access="public"
	>
	Eye distance
	</Field>
	<Field
		name="zeroparallax"
		type="Real32"
		cardinality="single"
		visibility="external"
		access="public"
	>
	zero paralax
	</Field>
	<Field
		name="syncSwap"
		type="bool"
		cardinality="single"
		visibility="external"
		defaultValue="true"
		access="public"
	>
	Sync swap if true
	</Field>
</FieldContainer>
